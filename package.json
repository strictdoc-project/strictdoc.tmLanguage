{
  "name": "strictdoc",
  "displayName": "StrictDoc Language Basics",
  "version": "0.1.0",
  "publisher": "StrictDoc",
  "description": "Support for the StrictDoc markup: https://github.com/strictdoc-project/",
  "homepage": "https://github.com/strictdoc-project/strictdoc.tmLanguage",
  "author": "Maryna Balioura <mettta@gmail.com>",
  "engines": {
    "vscode": "^1.76.0"
  },
  "license": "Apache 2.0",
  "categories": [
    "Programming Languages",
    "Language Packs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/strictdoc-project/strictdoc.tmLanguage.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "sdoc",
        "extensions": [
          ".sdoc"
        ],
        "aliases": [
          "StrictDoc",
          "sdoc"
        ],
        "firstLine": "^\\[DOCUMENT\\]$",
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [{
      "language": "sdoc",
      "scopeName": "source.sdoc",
      "path": "./syntaxes/sdoc.tmLanguage.json"
    }]
  }
}
